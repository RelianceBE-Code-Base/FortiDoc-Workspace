{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "digital-workspace-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/digitalWorkspace/DigitalWorkspaceWebPart.js",
          "manifest": "./src/webparts/digitalWorkspace/DigitalWorkspaceWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "@pnp/sp/webs": {
      "path": "node_modules/@pnp/sp/webs/dist/index.js",
      "globalName": "SP"
    }
  },
  "localizedResources": {
    "DigitalWorkspaceWebPartStrings": "lib/webparts/digitalWorkspace/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}